# vim: ft=sshconfig

ServerAliveInterval 60
SendEnv TERM COLORTERM HAS_POWERLINE_FONTS TURBO_MODE
AddKeysToAgent yes
Host github.com
  IdentityFile /home/th026106/.ssh/git_key
Host github.digitalglobe.com
  IdentityFile /home/th026106/.ssh/git_key
Host localhost
  SendEnv DBUS_SESSION_BUS_ADDRESS DISPLAY TMUX
